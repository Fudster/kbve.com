---
//<!--?         [Media]
import KBVE from '@l/KBVE.astro';
import Menu from '@l/Menu.astro';
import N from '@n/N.astro';
import Oracle from '@w/Oracle.astro';
//<!--*         [DATA]
let title = 'KBVE Media - Podcasts, Shows, Movies, Music and More!';
let description =
	'All the Media content that KBVE has compiled for your entertainment and pleasure!';
---

<KBVE title={title} description={description}>
	<Menu>
		<section class="py-6 sm:py-12">
			<div class="container p-6 mx-auto space-y-8">
				<div class="rounded-full space-y-2 text-center bg-offset p-4">
					<h2 class="text-3xl font-bold">KBVE Media</h2>
					<p class="font-serif text-sm dark:text-gray-400">Latest Media.</p>
				</div>

				<Oracle element="music" data="music" />
				<Oracle element="podcast" data="podcast" />
				<Oracle element="video" data="video" />
			</div>
		</section>
		<!-- <N ns="ads" template="midbanner" id="ezoic" /> -->
	</Menu>
</KBVE>
