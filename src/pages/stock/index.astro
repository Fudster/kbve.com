---
//<!--?       [Stock]
import KBVE from '@l/KBVE.astro';
import Menu from '@l/Menu.astro';
import N from '@n/N.astro';
import Oracle from '@w/Oracle.astro';
//<!--*       [DATA]
let title = 'Stock Analysis & Charts - KBVE.com';
let description =
	'Collection of documentation, references and information related to Stocks. Amazing charts and abstract details will be coming soon.';
---

<KBVE title={title} description={description}>
	<Menu>
		<section class="py-6 sm:py-12">
			<div class="container p-6 mx-auto space-y-8">
				<div class="rounded-full space-y-2 text-center bg-offset p-4">
					<h2 class="text-3xl font-bold">Stock Analysis & Charts</h2>
					<p class="font-serif text-sm dark:text-gray-400">
						Stock charts, data, information, analysis and reports.
					</p>
				</div>

				<Oracle element="stock" data="stock" />
			</div>
		</section>
		<N ns="ads" template="midbanner" id="ezoic" />
	</Menu>
</KBVE>
